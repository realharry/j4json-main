<?xml version="1.0" encoding="ISO-8859-1"?>

<project name="Maven" xmlns="http://maven.apache.org/DECORATION/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/DECORATION/1.0.0 http://maven.apache.org/xsd/decoration-1.0.0.xsd">

  <skin>
    <groupId>org.apache.maven.skins</groupId>
<!--
    <artifactId>maven-classic-skin</artifactId>
    <version>1.0</version>
-->
    <artifactId>maven-fluido-skin</artifactId>
    <version>1.3.0</version>
  </skin>
  <custom>
    <fluidoSkin>
      <topBarEnabled>true</topBarEnabled>
      <sideBarEnabled>true</sideBarEnabled>
      <copyrightStyle>pull-right</copyrightStyle>
      <googleSearch>
        <sitesearch>${project.url}</sitesearch>
      </googleSearch>
      <!-- break exec /www/www.apache.org/dyn/mirrors/mirrors.cgi -->
      <twitter>
        <user>minijson</user>
        <showUser>true</showUser>
        <showFollowers>false</showFollowers>
      </twitter>
    </fluidoSkin>
  </custom>

  <bannerLeft>
    <name>MiniJSON for Java</name>
    <src>http://www.minijson.org/images/minijson-logo-v1.png</src>
    <href>http://www.minijson.org/</href>
  </bannerLeft>
<!--
  <bannerRight>
    <src>http://maven.apache.org/images/maven-small.gif</src>
  </bannerRight>
-->

<!--
